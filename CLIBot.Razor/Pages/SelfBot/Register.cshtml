@page
@using CLIBot.Razor.Pages.SelfBot;
@model RegisterModel
@{
    ViewData["Title"] = "Register";
}

@section css
    {
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/boxicons.min.css">
}


